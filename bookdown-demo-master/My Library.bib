@article{siegel_fatal_2015,
	title = {Fatal Water Intoxication and Cardiac Arrest in Runners During Marathons: Prevention and Treatment Based on Validated Clinical Paradigms},
	volume = {128},
	issn = {00029343},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0002934315003538},
	doi = {10.1016/j.amjmed.2015.03.031},
	shorttitle = {Fatal Water Intoxication and Cardiac Arrest in Runners During Marathons},
	abstract = {Cerebral edema due to exercise-associated hyponatremia and cardiac arrest due to atherosclerotic heart disease cause rare marathonrelated fatalities in young female and middle-aged male runners, respectively. Studies in asymptomatic middle-aged male runners during races identified inflammation due to skeletal muscle injury after glycogen depletion as the shared underlying cause. Nonosmotic secretion of arginine vasopressin as a neuroendocrine stress response to rhabdomyolysis mediates hyponatremia as a variant of the syndrome of inappropriate antidiuretic hormone secretion. Fatal hyponatremic encephalopathy in young female runners was curtailed using emergent infusion of intravenous hypertonic (3\%) saline to reverse cerebral edema based on this paradigm, which treatment was arrived at through a consensus process within the medical community. An increasing frequency of cardiac arrest and sudden death has been identified in middle-aged male runners in two studies since the year 2000. Same-aged asymptomatic male physician-runners showed post-race elevations in interleukin-6 and C-reactive protein, which biomarkers predict acute cardiac events in healthy persons. Hypercoagulability with in vivo platelet activation and release of cardiac troponin and {NT}-pro-B-natriuretic peptide were also observed post-race in these same subjects. High short-term risk for atherothrombosis during races as shown by stratification of biomarkers in asymptomatic males may render non-obstructive coronary atherosclerotic plaques vulnerable to rupture. Pre-race aspirin usage in this high-risk subgroup is prudent based on conclusive evidence for preventing first acute myocardial infarctions in same-aged healthy male physicians. Based on validated clinical paradigms, taking a low-dose aspirin before a marathon and drinking to thirst during the race may avert preventable deaths in susceptible runners.},
	pages = {1070--1075},
	number = {10},
	journaltitle = {The American Journal of Medicine},
	shortjournal = {The American Journal of Medicine},
	author = {Siegel, Arthur J.},
	urldate = {2021-11-17},
	date = {2015-10},
	langid = {english},
	file = {siegel hyponatremia.pdf:/Users/lebertm/Documents/Articles/siegel hyponatremia.pdf:application/pdf},
}

